/**
 * 16-bit multiply by 2:
 * out = in*2 = in + in (arithmetic addition)
 */

CHIP ShiftLeft {
    IN a[16];
    OUT out[16];

    PARTS:
   	Add16(a = a, b = a, out = out);
}