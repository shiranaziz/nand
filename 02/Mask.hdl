 /**
 * Mask:
 * preserves the inserted value 
 * out = in 
 */

CHIP Mask {
    IN a;
    OUT out;

    PARTS:
    // Table of Truth:
    // Or(1,0) = 1
	// Or(0,0) = 0

    Or(a = a, b = false, out = out);
}


